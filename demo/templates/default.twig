<html>
<head>
    <title>{{ page.title }} :: {{ site.name }}</title>
    <link rel="canonical" href="{{ page.url(true) }}" />
</head>
<body>
    <h1>{{ page.title }}</h1>
    {{ page.date|date }}<br/>
    {{ page.url(true) }}
    
    <h2>Children</h2>
    {% for page in plaster.children %}
        <div>{{ page.link|raw }} {{ page.date|date }}</div>
    {% endfor %}
    
    <h2>Siblings</h2>
    {% for page in plaster.siblings %}
        <div>{{ page.link|raw }} {{ page.date|date }}</div>
    {% endfor %}
    
    <h2>Breadcrumb</h2>
    {% for page in plaster.breadcrumb %}
        <div>{{ page.link|raw }} {{ page.date|date }}</div>
    {% endfor %}
    
</body>
</html>